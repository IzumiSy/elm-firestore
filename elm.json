{
    "type": "package",
    "name": "IzumiSy/elm-firestore",
    "summary": "A library for integrating your app with Firestore in Elm",
    "license": "MIT",
    "version": "1.0.0",
    "exposed-modules": [
        "Firestore",
        "Firestore.Config.APIKey",
        "Firestore.Config.DatabaseId",
        "Firestore.Config.ProjectId",
        "Firestore.Document",
        "Firestore.Document.Fields",
        "Firestore.Transaction",
        "Firestore.Types",
        "Firestore.Types.Reference",
        "Firestore.Types.Timestamp",
        "Firestore.Types.Geopoint",
        "Firestore.Types.Bool",
        "Firestore.Types.Bytes",
        "Firestore.Types.Int",
        "Firestore.Types.List",
        "Firestore.Types.Map",
        "Firestore.Types.Null",
        "Firestore.Types.String"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "NoRedInk/elm-json-decode-pipeline": "1.0.0 <= v < 2.0.0",
        "elm/bytes": "1.0.8 <= v < 2.0.0",
        "elm/core": "1.0.2 <= v < 2.0.0",
        "elm/http": "2.0.0 <= v < 3.0.0",
        "elm/json": "1.1.3 <= v <= 2.0.0",
        "elm/regex": "1.0.0 <= v < 2.0.0",
        "elm/time": "1.0.0 <= v < 2.0.0",
        "lukewestby/elm-string-interpolate": "1.0.4 <= v < 2.0.0",
        "rtfeldman/elm-iso8601-date-strings": "1.1.3 <= v < 2.0.0"
    },
    "test-dependencies": {
        "elm/random": "1.0.0 <= v < 2.0.0",
        "elm/url": "1.0.0 <= v < 2.0.0",
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}